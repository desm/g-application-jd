{
  "family": "gumroad-family",
  "requiresCompatibilities": ["FARGATE"],
  "executionRoleArn": "arn:aws:iam::711112755670:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "runtimePlatform": {
    "operatingSystemFamily": "LINUX",
    "cpuArchitecture": "X86_64"
  },
  "cpu": "256",
  "memory": "512",
  "containerDefinitions": [
    {
      "name": "rails-app-container",
      "image": "711112755670.dkr.ecr.us-east-1.amazonaws.com/gumroad-jd:latest",
      "portMappings": [
        {
          "containerPort": 80,
          "protocol": "tcp"
        },
        {
          "containerPort": 443,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "secrets": [
        {
          "name": "RAILS_MASTER_KEY",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:711112755670:secret:rails-master-key-8wP0FT"
        }
      ]
    }
  ]
}
